package com.company;

import java.util.*;

public class Main
{
    public static void main(String[] args)
    {
        Scanner in = new Scanner(System.in);
        System.out.print("Enter a number ");
        int i,factorial=1;
        int number=in.nextInt();
        for(i=2;i<=number;i++)
            factorial=factorial*i;
        System.out.println(factorial);
    }

}


